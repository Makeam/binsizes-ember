<hr>
<div>
    <h3>All configurations</h3>
    <table class="table">
    <tbody>

    {{#each model as |binsize|}}
        <tr>
            <td>{{binsize.id}} - {{binsize.const}}</td>
            <td>
            {{#each binsize.packages as |package|}}
                {{package.val}}{{package.unit}} - {{package.percent}}%
            {{/each}}
            </td>
            <td>{{#link-to 'binsize' binsize}}Edit{{/link-to}}</td>
            <td><a href="" {{action "deleteBinsize" binsize}}>Delete</a></td>
        </tr>
    {{/each}}
    </tbody>
    </table>
</div>
<hr>
<div>{{#link-to 'create'}}Add bin{{/link-to}}</div>
